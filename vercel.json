{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/static/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/rss",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/xml"
        }
      ]
    }
  ]
}
